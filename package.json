{
  "name": "mibici-find-bike",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc",
    "test": "vitest run",
    "stress:nearby": "tsx test/stress/nearby.stress.ts",
    "stress:mixed": "tsx test/stress/mixed.stress.ts",
    "migrate": "tsx src/db/migrate.ts",
    "seed": "tsx src/db/seed.ts",
    "ci:db": "docker compose up -d db",
    "ci:waitdb": "node -e \"const net=require('net'); const port=process.env.DB_PORT||5432; const host=process.env.DB_HOST||'127.0.0.1'; let n=0; const t=setInterval(()=>{n++; const s=net.createConnection({host,port},()=>{console.log('db ready'); clearInterval(t); s.end(); process.exit(0);}); s.on('error',()=>{ if(n>60){console.error('db not ready'); process.exit(1);} });},1000);\"",
    "ci:prep": "npm run migrate && npm run seed",
    "ci:test": "npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "pg": "^8.17.2",
    "pino-pretty": "^13.1.3",
    "postgis": "^1.0.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "autocannon": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "undici": "^7.19.2",
    "vitest": "^4.0.18"
  }
}
